{
	"name": "Rhino",
	"version": "1.0",
	"description": "Project manager for developers",
	"manifest_version": 2,
	"permissions": ["activeTab", "bookmarks", "storage"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Rhino"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"css": ["content.css"]
		}
	]
}
