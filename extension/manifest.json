{
	"name": "Rhino",
	"version": "1.0",
	"description": "Project manager for developers",
	"manifest_version": 2,
	"permissions": ["activeTab", "bookmarks", "storage"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Bookmark-Manager"
	},
	"background": {
		"service_worker": "background.js"
	}
}
